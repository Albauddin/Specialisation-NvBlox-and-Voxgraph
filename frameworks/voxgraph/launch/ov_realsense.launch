<launch>
  <node name="ov_msckf" pkg="ov_msckf" type="run_subscribe_msckf"
        output="screen" clear_params="true" required="true">
    <param name="verbosity"   type="string" value="INFO"/>
    <param name="config_path" type="string" value="$(find ov_msckf)/../config/euroc_mav/estimator_config.yaml"/>
    <param name="use_stereo"  type="bool"   value="true"/>
    <param name="max_cameras" type="int"    value="2"/>

    <!-- NOTE: relative 'from' names (no leading slash) -->
    <remap from="cam0/image_raw"   to="/camera/infra1/image_rect_raw"/>
    <remap from="cam0/camera_info" to="/camera/infra1/camera_info"/>
    <remap from="cam1/image_raw"   to="/camera/infra2/image_rect_raw"/>
    <remap from="cam1/camera_info" to="/camera/infra2/camera_info"/>
    <remap from="imu0"             to="/camera/imu"/>

  </node>
  <node type="rviz" name="rviz" pkg="rviz" args="-d $(find ov_msckf)/launch/display.rviz" />
</launch>

